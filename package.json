{
  "name": "vue-fanscc-table",
  "version": "0.1.3",
  "description": "后台管理系统表格公用组件",
  "author": "fanscky",
  "private": false,
  "main": "lib/vue-fanscc-table.umd.min",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "mock": "vue-cli-service serve --mode mock",
    "make": "node ./build/mk.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name vue-fanscc-table --dest lib src/plug/index.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.3.2",
    "element-ui": "^2.12.0",
    "fs": "0.0.1-security",
    "fs-jetpack": "^2.2.3",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "json-server": "^0.15.1",
    "nodemon": "^1.19.4",
    "path": "^0.12.7",
    "shelljs": "^0.8.3",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0,
      "no-debugger": 0,
      "no-undef": 0,
      "eqeqeq": 2
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}